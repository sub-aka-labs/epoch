{
  "name": "@privy-io/react-auth",
  "version": "3.12.0",
  "description": "React client for the Privy Auth API",
  "keywords": [
    "authentication",
    "authorization",
    "identity",
    "privacy",
    "privy",
    "user data",
    "web3"
  ],
  "homepage": "https://docs.privy.io",
  "bugs": {
    "url": "https://privy.io/slack"
  },
  "license": "Apache-2.0",
